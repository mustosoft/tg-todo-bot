{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "never",
                "json": "always"
            }
        ],
        "indent": [
            "error",
            4
        ]
    }
}